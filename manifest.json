{
    "manifest_version": 2,
    "name": "Stack Overflow Modern",
    "version": "0.1",
    "description": "Makes Stack Overflow usable",
    "content_scripts": [
        {
            "matches": [
                "https://stackoverflow.com/*"
            ],
            "css": [
                "content.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    }
}